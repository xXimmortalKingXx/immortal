local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Scripts HUB", "DarkTheme")
-- Rebirth Champions X
local RCX = Window:NewTab("Rebirth Champions X")

local RCXSection = RCX:NewSection("Rebirth Champions X")
RCXSection:NewButton("Rebirth Champions Script #1", "Click to open script!", function()
    print("Clicked")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/uzu01/lua/main/loader.lua"))()
end)

-- Devious Lick Simulator
local DLS = Window:NewTab("Devious Lick Simulator")

local DLSSection = DLS:NewSection("Devious Lick Simulator")
DLSSection:NewButton("Devious Lick Simulator Script #1", "Click to open script!", function()
    print("Clicked")
    loadstring(game:HttpGet"https://thedragonslayer2.github.io")()
end)

DLSSection:NewToggle("Auto Open Egg", "This Script Opens BEST egg in game now. i change it it", function(state)
    if state then
        print("Toggle On")while true do
        game:GetService("ReplicatedStorage").Events.OpenEgg:InvokeServer("Common Egg",3) -- u can change egg to open
        wait()
        end
    else
        print("Toggle Off")
    end
end)
